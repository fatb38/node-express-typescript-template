{
  "name": "node-express-typescript-template",
  "version": "0.1.0",
  "private": false,
  "description": "A from scratch template to create server app with NodeJS / Express / TypeScript",
  "author": "fatb38",
  "main": "src/app.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fatb38/node-express-typescript-template.git"
  },
  "scripts": {
