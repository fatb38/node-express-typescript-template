    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "license": "MIT"
}
